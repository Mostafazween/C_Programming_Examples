#include <stdio.h>
int main()
{
   unsigned int row,col,length,width;
   printf("Enter Length Of Square: ");
   scanf("%u",&length);
   printf("Enter Width Of Square: ");
   scanf("%u",&width);
   for(row=0;row<length;row++)
   {
       for(col=0;col<width;col++)
       {
           if(row==0||row==length-1||col==0||col==width-1)
           {
                printf("* ");
           }
           else
           {
               printf("  ");
           }


       }
       printf("\n");
   }
    return 0;
}
